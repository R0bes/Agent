# Domain-Konfiguration
DOMAIN=localhost

# Traefik-Konfiguration
TRAEFIK_HTTP_REDIRECT=true  # true = HTTP zu HTTPS weiterleiten, false = nur HTTP
TRAEFIK_TLS=true            # true = HTTPS aktivieren, false = nur HTTP

# Bot-Konfiguration (wenn aktiviert)
TELEGRAM_BOT_TOKEN=your_token_here
TELEGRAM_WEBHOOK_URL=https://telegram.your-domain.com/webhook

# Entwicklungsumgebung
ENVIRONMENT=development  # development, staging, production
